{
  "compilerOptions": {
    "declaration": true,
    "importHelpers": true,
    "module": "commonjs",
    "outDir": "lib",
    "strict": true,
    "target": "es6",
    "rootDirs": [
      "./src"
    ],
    "typeRoots": [
      "node_modules/@types"
    ],
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "lib": [
      "ES2015"
    ]
  }
}
