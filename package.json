{
  "name": "ogit",
  "description": "A lazy developer's Git CLI made simple",
  "version": "1.20.0",
  "author": "shakil.siraj@gmail.com",
  "bin": {
    "ogit": "./bin/run"
  },
  "bugs": "https://bitbucket.org/ssiraj/ogit/issues",
  "dependencies": {
    "@oclif/command": "^1.5.4",
    "@oclif/config": "^1.9.0",
    "@oclif/plugin-help": "^2.1.3",
    "chalk": "^2.4.1",
    "columnify": "^1.5.4",
    "inquirer": "^6.2.0",
    "json-object-mapper": "^1.5.2",
    "reflect-metadata": "^0.1.12",
    "simple-git": "^1.107.0",
    "ssh-keygen2": "^0.1.0",
    "terminal-link": "^1.1.0",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.19.4",
    "@oclif/tslint": "^3.1.1",
    "@types/chai": "^4.1.7",
    "@types/inquirer": "0.0.43",
    "@types/jest": "^23.3.9",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.5",
    "@types/uuid": "^3.4.4",
    "cli-ux": "^4.9.3",
    "globby": "^8.0.1",
    "jest": "^23.6.0",
    "prettier": "^1.15.2",
    "ts-jest": "^23.1.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^2.8.3",
    "uuid": "^3.3.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://bitbucket.org/ssiraj/ogit",
  "keywords": [
    "git",
    "git cli"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "ogit",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "https://bitbucket.org/ssiraj/ogit",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p . -t stylish",
    "prettier": "prettier --write src/**/*.ts",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "jest --runInBand",
    "test-watch": "jest --watch --no-cache --runInBand --colors",
    "linter-autofix": "tslint -p . --fix",
    "jest-clear-cache": "jest --clearCache",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
