{
  "name": "ogit",
  "description": "A lazy developer's Git CLI made simple. Makes using git on cloud IDEs (i.e. C9) a walk in the park.",
  "version": "1.31.0",
  "author": "shakil.siraj@gmail.com",
  "bin": {
    "ogit": "./bin/run.js"
  },
  "bugs": "https://github.com/shakilsiraj/ogit/issues",
  "dependencies": {
    "@andrewwlane/ssh-keygen2": "^1.1.4",
    "@oclif/command": "^1.8.22",
    "@oclif/config": "^1.18.8",
    "@oclif/plugin-autocomplete": "^2.1.5",
    "@oclif/plugin-help": "^5.2.8",
    "@oclif/plugin-not-found": "^2.3.22",
    "@oclif/plugin-warn-if-update-available": "^2.0.31",
    "chalk": "^5.2.0",
    "cli-ux": "^4.9.3",
    "columnify": "^1.6.0",
    "inquirer": "^8.2.5",
    "inquirer-autocomplete-prompt": "^3.0.0",
    "json-object-mapper": "^1.7.1",
    "reflect-metadata": "^0.1.13",
    "simple-git": "^3.17.0",
    "terminal-link": "^3.0.0",
    "tslib": "^2.5.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.10",
    "@oclif/tslint": "^3.1.1",
    "@types/chai": "^4.3.4",
    "@types/inquirer": "9.0.3",
    "@types/jest": "^29.4.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.15.3",
    "@types/uuid": "^9.0.1",
    "globby": "^11",
    "jest": "^29.5.0",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tslint": "^5.20.1",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/shakilsiraj/ogit",
  "keywords": [
    "git",
    "git cli",
    "cli",
    "Source Control",
    "Version Control",
    "VCS",
    "IDE",
    "Online",
    "Cloud"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "ogit",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-warn-if-update-available"
    ]
  },
  "repository": "https://github.com/shakilsiraj/ogit",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p . -t stylish",
    "prettier": "prettier --write src/**/*.ts",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme",
    "test": "jest --runInBand",
    "test-watch": "jest --watch --no-cache --runInBand --colors",
    "linter-autofix": "tslint -p . --fix",
    "jest-clear-cache": "jest --clearCache",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "type": "commonjs"
}
